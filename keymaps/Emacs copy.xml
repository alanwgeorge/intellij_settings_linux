<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="alt p" />
    <keyboard-shortcut first-keystroke="shift insert" />
    <keyboard-shortcut first-keystroke="ctrl y" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt s" />
    <keyboard-shortcut first-keystroke="alt comma" />
  </action>
</keymap>